{
  "name": "cognicity-twitter-dm-lambda",
  "version": "0.0.4",
  "description": "Twitter DM bot runnning on AWS lambda",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --fix src",
    "mocha": "nyc mocha --compilers js:babel-core/register src/test/index",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test": "npm run lint && npm run mocha",
    "build": "./node_modules/.bin/babel src -d bin && cp src/lib/*.json bin/lib/"
  },
  "keywords": [
    "bot",
    "twitter"
  ],
  "author": "Tomas Holderness",
  "license": "GPL-3.0",
  "dependencies": {
    "@urbanriskmap/cognicity-bot-core": "^1.0.3",
    "dotenv": "^6.0.0",
    "joi": "^13.4.0",
    "oauth": "^0.9.15",
    "request": "^2.87.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-preset-es2015": "^6.24.1",
    "coveralls": "^3.0.2",
    "eslint": "^5.0.1",
    "eslint-config-google": "^0.9.1",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "unit.js": "^2.0.0"
  }
}
